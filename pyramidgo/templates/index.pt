<!doctype html>
<html>
<head>
  <title>Tic Tac Toe</title>
  <meta name="viewport" content="" />
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <link rel="stylesheet" href="${request.static_url('pyramidgo:static/main.css')}" type="text/css">
  <script src="${request.static_url('pyramidgo:static/test.js')}" type="text/javascript"></script>
</head>
<body onload="init();">
<div id="left">  
<div id="players">
  <div id="X" class="">Player X</div>
  <div id="O" class="">Player O</div>
  <input type="button" class="button" onclick="newGame();" value="New Game">
</div>
<table>
  <tr>
    <td id="0"></td>
    <td id="1"></td>
    <td id="2"></td>
  </tr>
  <tr>
    <td id="3"></td>
    <td id="4"></td>
    <td id="5"></td>
  </tr>
  <tr>
    <td id="6"></td>
    <td id="7"></td>
    <td id="8"></td>
  </tr>
</table>
<div id="winner">Winner</div>
</div>
<div id="right">
  <div>&nbsp</div>
  <div>&nbsp</div>
  <div class="div-table">
   <div class="div-table-caption">Game History</div>
   <div class="div-table-row">
    <div class="div-table-col" id="h0">1</div>
    <div class="div-table-col" id="h1">2</div>
   </div>
   <div class="div-table-row">
    <div class="div-table-col" id="h2">3</div>
    <div class="div-table-col" id="h3">4</div>
   </div>
  </div>
</div>
</body>
</html>